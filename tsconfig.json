{
  "extends": "astro/tsconfigs/strict",
  "include": ["src/**/*.ts", "src/env.d.ts", "shims-vue.d.ts"],
  "compilerOptions": {
    "target": "es2021",
    "module": "es2015",
    "moduleResolution": "node",
    "lib": ["es2021", "dom"],
    "declaration": true,
    "declarationMap": true,
    "experimentalDecorators": true,
    "useDefineForClassFields": false
  }
}