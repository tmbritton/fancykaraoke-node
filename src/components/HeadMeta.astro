---
interface Props {
	title: string;
}

const { title } = Astro.props;
---
<head>
  <meta charset="UTF-8" />
  <meta name="description" content="The greatest karaoke app in the multiverse." />
  <meta name="viewport" content="width=device-width" />
  <link rel="icon" type="image/svg+xml" href="/icons/fk.svg" />
  <meta name="generator" content={Astro.generator} />
  <title>{title}</title>
</head>